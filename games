-- // COPYS DISCORD INVITE
Extra:Button{
	Name = "Copy my miau",
	Description = nil,
	Callback = function()
        setclipboard('miau')
    end
}

-- // REMOVE NOTIFICATIONS
game:GetService("Players").LocalPlayer.PlayerGui.Display.Notifications:Destroy() -- // DESTROYS THE GEY ASS NOTIFICATIONS 
